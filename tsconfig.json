{
	"compilerOptions": {
	  "module": "Node16",
	  "moduleResolution": "node16",
	  "target": "ES2022",
	  "outDir": "out",
	  "lib": ["ES2022"],
	  "sourceMap": true,
	  "rootDir": "src",
	  "strict": false,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true
	},
	"include": [
	  "src"
	],
	"exclude": [
	  "node_modules",
	  ".vscode-test"
	]
  }
  